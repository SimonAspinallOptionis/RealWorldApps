        </ul>
    </li>
</ul>
<div id="myTabContent" class="tab-content">
    <div class="tab-pane fade active in" id="welcome">
        <div class="page-header">
            <b>Welcome</b>
        </div>
        <div class="panel-body">
            <div class="text-left">
                Welcome to the Cheshire Wire White website
            </div>
            <div class="text-left">
                This website holds all the information you might need to know about Cheshire Wire White!
            </div>
        </div>
    </div>
    <div class="tab-pane fade" id="news">
        @foreach (var story in ViewBag.NewsCollection)
        {
            <div class="page-header text-capitalize">
                <b>@story.Headline</b>
            </div>
            <div class="panel-body text-left">
                @story.Body
            </div>
        }
    </div>
    <div class="tab-pane fade" id="under13WhiteFixture">
        <div class="page-header text-capitalize">
            <b>@ViewBag.Under13WhiteFixture.Organisation&nbsp;@ViewBag.Under13WhiteFixture.OpponentName</b>
        </div>
        <div class="panel-body">
            <b>Tip Off:</b>&nbsp;@ViewBag.Under13WhiteFixture.TipOff<br />
            <b>Location:</b>&nbsp;@ViewBag.Under13WhiteFixture.AddressLine1,&nbsp;@ViewBag.Under13WhiteFixture.TownCity
