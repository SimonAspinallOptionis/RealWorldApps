<div id="myTabContent" class="tab-content">
    <div class="tab-pane fade active in" id="welcome">
        <div class="page-header">
            <b>Welcome</b>
        </div>
        <div class="panel-body">
            <div class="text-left">
                Welcome to the Cheshire Wire White website
            </div>
            <div class="text-left">
                This website holds all the information you might need to know about Cheshire Wire White!
            </div>
        </div>
    </div>
    <div class="tab-pane fade" id="news">
        @foreach (var item in Model)
        {
            <div class="page-header text-capitalize">
                <b>@item.Headline</b>
            </div>
            <div class="panel-body text-left">
                @item.Body
            </div>
        }
    </div>
